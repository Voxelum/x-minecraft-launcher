{
    "type": "object",
    "properties": {
        "caches": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/CachedFTBModpackVersionManifest"
            }
        }
    },
    "required": [
        "caches"
    ],
    "definitions": {
        "CachedFTBModpackVersionManifest": {
            "type": "object",
            "properties": {
                "iconUrl": {
                    "type": "string"
                },
                "projectName": {
                    "type": "string"
                },
                "authors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/FTBAuthor"
                    }
                },
                "files": {
                    "description": "The list of file in this modpack",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/FTBFile"
                    }
                },
                "specs": {
                    "$ref": "#/definitions/FTBSpecs"
                },
                "targets": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/FTBTarget"
                    }
                },
                "installs": {
                    "type": "number"
                },
                "plays": {
                    "type": "number"
                },
                "refreshed": {
                    "type": "number"
                },
                "status": {
                    "type": "string"
                },
                "changelog": {
                    "type": "string"
                },
                "parent": {
                    "type": "number"
                },
                "id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "updated": {
                    "type": "number"
                }
            },
            "required": [
                "authors",
                "changelog",
                "files",
                "iconUrl",
                "id",
                "installs",
                "name",
                "parent",
                "plays",
                "projectName",
                "refreshed",
                "specs",
                "status",
                "targets",
                "type",
                "updated"
            ]
        },
        "FTBAuthor": {
            "type": "object",
            "properties": {
                "website": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "updated": {
                    "type": "number"
                }
            },
            "required": [
                "id",
                "name",
                "type",
                "updated",
                "website"
            ]
        },
        "FTBFile": {
            "type": "object",
            "properties": {
                "version": {
                    "type": "string"
                },
                "path": {
                    "description": "The file path relative to the instance root (without name)",
                    "type": "string"
                },
                "url": {
                    "description": "Download url",
                    "type": "string"
                },
                "sha1": {
                    "type": "string"
                },
                "size": {
                    "type": "number"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "clientonly": {
                    "type": "boolean"
                },
                "serveronly": {
                    "type": "boolean"
                },
                "optional": {
                    "type": "boolean"
                },
                "id": {
                    "type": "number"
                },
                "curseforge": {
                    "type": "object",
                    "properties": {
                        "project": {
                            "type": "number"
                        },
                        "file": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "file",
                        "project"
                    ]
                },
                "name": {
                    "description": "The file name",
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "updated": {
                    "type": "number"
                }
            },
            "required": [
                "clientonly",
                "id",
                "name",
                "optional",
                "path",
                "serveronly",
                "sha1",
                "size",
                "tags",
                "type",
                "updated",
                "version"
            ]
        },
        "FTBSpecs": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "number"
                },
                "minimum": {
                    "type": "number"
                },
                "recommended": {
                    "type": "number"
                }
            },
            "required": [
                "id",
                "minimum",
                "recommended"
            ]
        },
        "FTBTarget": {
            "description": "The target is like the required runtime.\n\nFor example, name: \"minecraft\", version: \"1.16.4\", type: \"game\" is Minecraft 1.16.4\nname: \"forge\", version: \"31.1.30\", type: \"modloader\" is Minecraft 1.16.4",
            "type": "object",
            "properties": {
                "version": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "updated": {
                    "type": "number"
                }
            },
            "required": [
                "id",
                "name",
                "type",
                "updated",
                "version"
            ]
        }
