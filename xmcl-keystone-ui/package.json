        "vue-i18n": "^7.8.1",
        "vue-router": "^3.5.2",
        "vue-virtual-scroll-list": "^2.3.3",
        "vuetify": "^1.5.24",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "@vue/devtools": "^5.3.4",
        "@vue/runtime-dom": "latest",
        "chalk": "^4.1.2",
        "cross-env": "^5.2.1",
        "esbuild": "^0.12.25",
        "eslint": "~7",
