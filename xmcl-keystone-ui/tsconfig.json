{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "/@/*": [
        "./src/*"
      ],
      "~main/*": [
        "./src/windows/main/*"
      ],
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "types": [
      "vite/client",
      "vite-plugin-pwa/client",
      "unplugin-vue2-script-setup/types",
      "unplugin-vue2-script-setup/ref-macros",
      "@xmcl/runtime-api/client",
      "@xmcl/runtime-api/monitorClient",
      "@modyfi/vite-plugin-yaml/modules"
    ]
  },
  "vueCompilerOptions": {
    "target": 2,
    "experimentalSuppressUnknownJsxPropertyErrors": false
  },
  "exclude": [
    "node_modules",
    "../xmcl-runtime-api"
  ]
}