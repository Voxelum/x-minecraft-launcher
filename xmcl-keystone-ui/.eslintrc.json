{
    "globals": {
        "taskMonitor": true,
        "resourceMonitor": true,
        "serviceChannels": true,
        "windowController": true,
        "appsHost": true,
        "gameOutput": true
    },
    "extends": [
        "../.eslintrc.json",
        "plugin:vue/recommended"
    ],
    "parser": "vue-eslint-parser",
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    }
}