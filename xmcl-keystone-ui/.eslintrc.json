{
    "globals": {
        "taskMonitor": true,
        "resourceMonitor": true,
        "serviceChannels": true,
        "windowController": true,
        "appsHost": true,
        "bootstrap": true,
        "gameMonitor": true,
        "defineProps": true,
        "withDefaults": true,
        "defineEmits": true,
        "defineExpose": true
    },
    "extends": [
        "../.eslintrc.json",
        ".eslintrc-auto-import.json",
        "plugin:vue/recommended"
    ],
    "parser": "vue-eslint-parser",
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "rules": {
        "vue/no-mutating-props": "off",
        "vue/multi-word-component-names": "off"
    }
}