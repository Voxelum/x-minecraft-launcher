{
    "name": "ilauncher",
    "productName": "ILauncher",
    "version": "0.0.0",
    "typings": "typings.d.ts",
    "author": "",
    "description": "A great minecraft launcher in the feature",
    "license": "MIT",
    "main": "./dist/electron/main.js",
    "scripts": {
        "build": "node scripts/build.js && electron-builder",
        "build:dir": "node scripts/build.js && electron-builder --dir",
        "build:clean": "cross-env BUILD_TARGET=clean node scripts/build.js",
        "dev": "node scripts/dev-runner.js",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config scripts/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config scripts/webpack.renderer.config.js",
        "lint": "eslint --ext .js,.vue src",
        "lint:fix": "eslint --ext .js,.vue --fix src",
        "test": "npm run unit",
        "unit": "node test/test-runner.js && mocha dist/test/unit.js",
        "postinstall": ""
    },
    "build": {
        "productName": "ilauncher",
        "appId": "net.infstudio.ilauncher",
        "directories": {
            "output": "build"
        },
        "files": [
            "dist/electron/**/*"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {},
        "win": {},
        "linux": {}
    },
    "dependencies": {
        "fast-html-parser": "^1.0.1",
        "fs-extra": "^4.0.0",
        "iconv-lite": "^0.4.18",
        "jszip": "^3.1.5",
        "treelike-task": "0.0.7",
        "ts-minecraft": "^4.0.0",
        "uuid": "^3.2.1",
        "vue": "^2.5.16",
        "vue-electron": "^1.0.6",
        "vue-i18n": "^7.0.4",
        "vue-router": "^3.0.1",
        "vuetify": "^1.2.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^7.2.3",
        "babel-generator": "^6.26.1",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "babili-webpack-plugin": "^0.1.2",
        "cfonts": "^1.1.3",
        "chalk": "^1.1.3",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.11",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^1.7.5",
        "electron-builder": "^19.19.1",
        "electron-debug": "^1.4.0",
        "electron-devtools-installer": "^2.2.0",
        "electron-packager": "^8.5.0",
        "electron-rebuild": "^1.1.3",
        "eslint": "^5.5.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-plugin-html": "^4.0.5",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-vue": "^4.7.1",
        "express": "^4.16.3",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "inject-loader": "^3.0.0",
        "mini-css-extract-plugin": "^0.4.2",
        "mocha": "^5.2.0",
        "multispinner": "^0.2.1",
        "style-loader": "^0.21.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "three": "^0.86.0",
        "three-orbit-controls": "^82.1.0",
        "url-loader": "^0.5.9",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^15.2.4",
        "vue-style-loader": "^4.1.0",
        "vue-template-compiler": "^2.5.16",
        "vuedraggable": "^2.16.0",
        "webpack": "^4.15.1",
        "webpack-dev-middleware": "^3.1.4",
        "webpack-dev-server": "^3.2.1",
        "webpack-hot-middleware": "^2.22.2"
    }
}