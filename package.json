    "jest": {
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        },
        "moduleNameMapper": {
            "^@universal(.*)$": "<rootDir>/src/universal$1",
            "^@main(.*)$": "<rootDir>/src/main$1"
        },
        "moduleFileExtensions": [
            "ts",
            "js",
            "json"
        ],
        "modulePaths": [
            "src"
        ],
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "src/.*spec\\.ts",
        "transform": {
            "^.+(?!\\.d)\\.ts$": "ts-jest"
        }
    }
